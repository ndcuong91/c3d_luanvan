train_net: "%s" 
# test_net: "we don't use testing on the fly"
test_iter: 100
test_interval: 0
base_lr: 0.0001
momentum: 0.9
weight_decay: 0.0005
lr_policy: "step"
gamma: 0.01
stepsize: 200
# Display every ... iterations 
display: 20
# The maximum number of iterations
max_iter: %s
# snapshot intermediate results
snapshot: %s
snapshot_prefix: "%s"
# solver mode: CPU or GPU
solver_mode: GPU
device_id: 0
